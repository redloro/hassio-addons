{
  "name": "RadioRA Classic SmartThings Gateway",
  "version": "1.0",
  "slug": "lutron-radiora-classic",
  "description": "This add-on installs the latest Lutron RadioRA Classic/V1 SmartThings Gateway by Homemations",
  "url":"https://github.com/rsnodgrass/hassio-addons/tree/master/lutron-radiora-classic",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "8080/tcp": 8333
  },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "options": {
    "tty": "/dev/ttyAMA0"
  },
  "schema": {
    "tty": "str"
  },
  "environment": {
    "SERIAL_TTY": "/dev/ttyAMA0"
  },
  "host_network": false,
  "privileged": [ SYS_RAWIO ],
  "gpio": true
}
