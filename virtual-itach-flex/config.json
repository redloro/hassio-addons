{
  "name": "Virtual iTach Flex Serial",
  "version": "0.0.1",
  "slug": "virtual-itach-flex",
  "description": "Emulator for iTach Flex serial connector",
  "url": "https://github.com/rsnodgrass/hassio-addons/tree/master/virtual-itach-flex",
  "startup": "services",
  "boot": "auto",
  "ports": {
    "80/tcp": 80,
    "5999/tcp": 5999,
    "9131/udp": 9131
  },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "auto_uart": true,
  "map": [
    "config:ro"
  ],
  "hassio_role": "default",
  "host_network": false,
  "gpio": true,
  "auto_uart": true,
  "devices": [ "/dev/ttyAMA0:/dev/ttyAMA0:rwm",
	       "/dev/ttyUSB0:/dev/ttyUSB0:rwm"
  	     ],
  "privileged": [ "SYS_RAWIO" ]
}
